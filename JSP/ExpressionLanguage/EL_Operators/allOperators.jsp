<html>
    <head></head>
    <body>

        <h1>EL Operators...</h1>
        <table>

            <tr>
                <th>\${2}</th>
                <th>${2}</th>
            </tr>

            <tr>
                <th>\${2+2}</th>
                <th>${2+2}</th>
            </tr>

            <tr>
                <th>\${2/2}</th>
                <th>${2/2}</th>
            </tr>

            <tr>
                <th>\$(5 % 2)</th>
                <th>${5 % 2}</th>
            </tr>

            <tr>
                <th>\${10 == 11}</th>
                <th>${10 == 11}</th>
            </tr>

            <tr>
                <th>\${10 != 11}</th>
                <th>${ 10 != 11}</th>
            </tr>

            <tr>
                <th>\${5 < 10}</th>
                <th>${5 < 10}</th>
            </tr>

            <tr>
                <th>\${5 > 10}</th>
                <th>${5 > 10}</th>
            </tr>

            <tr>
                <th>\${5 <= 5}</th>
                <th>${5 <= 5}</th>
            </tr>

            <tr>
                <th>\${10 >= 5}</th>
                <th>${10 >= 5}</th>
            </tr>

            <tr>
                <th>\${true && false}</th>
                <th>${true && false}</th>
            </tr>

            <tr>
                <th>\${true || false}</th>
                <th>${true || false}</th>
            </tr>

            <tr>
                <th>\${!false}</th>
                <th>${!false}</th>
            </tr>
        </table>

    </body>
</html>